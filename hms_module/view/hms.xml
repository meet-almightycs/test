<?xml version="1.0" encoding="utf-8"?>
<odoo>
<record id="rounding_tree_view" model="ir.ui.view">
            <field name="name">hms.doctor.tree</field>
            <field name="model">hms.doctor</field>
            <field name="arch" type="xml">
                <tree string="Tree">
                    <field name="name"/>       
                    <field name="doctor_type"/>
                </tree>
            </field>
        </record>

       
        <record id="action_account_fiscal_year_form" model="ir.ui.view">
            <field name="name"></field>
            <field name="model">hms.doctor</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                          
                        <group>
                            <group>
                                <field name="name" />
                                <field name="doctor_id"/>
                                <field name="primary_surgeon"/>
                                <field name="consultation_physician"/>
                                <field name="amount"/>
                                <field name="doctor_type"/>
                                <field name="doctor_name_id"/>
                                <field name="note"/>
                            </group>

                               
                            <group>
                                 <field name="government_id"/>
                                 <field name="specialty"/> 
                                 <field name="photo" widget='image' class="oe_avatar"/>

                            </group>
                        </group>
                        <notebook> 
                              <page string="General Information">
                                <group>
                                    <group>
                                        <field name="address"/>
                                        <field name="allowed_online_booking"/>    
                                    </group>
                                    <group>
                                        <field name="mobile_No"/>
                                        
                                    </group>    
                                </group>
                              </page>
                               <page string="Other">
                               
                                    <field name="family">
                                         <tree string="Tree">
                                            <field name="name"/>       
                                            <field name="family"/>
                                            <field name="doctor"/>
                                         </tree>
                                    </field>
                                
                              </page>
                        </notebook>

                    </sheet>
                </form>
            </field>
        </record>

           <record id="rounding_list_action" model="ir.actions.act_window">
            <field name="name">name</field>
            <field name="res_model">hms.doctor</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">Create the first cash rounding</p>
            </field>
        </record>


        <menuitem id="menu_action_rounding_form_view" action="rounding_list_action"
                  sequence="4"/>
</odoo>